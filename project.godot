; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="StepToStep"
run/main_scene="res://Demo/main_menu.tscn"
config/features=PackedStringArray("4.1", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

SceneTransition="*res://scenes/scene_transition.tscn"
SaveSystem="*res://scenes/save_system.tscn"

[display]

window/size/viewport_width=700
window/size/viewport_height=1000
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"
window/handheld/orientation=1

[dotnet]

project/assembly_name="StepToStep"

[import]

enable_blend_file_import=false

[layer_names]

2d_render/layer_1="Background 1"
2d_render/layer_2="Background 2"
2d_render/layer_3="Background 3"
2d_render/layer_4="Foreground 1"
2d_render/layer_5="Foreground 2"
2d_render/layer_6="Entities 1"
2d_render/layer_7="Entities 2"
2d_render/layer_8="Foreground 3"
2d_render/layer_9="Entities 3"
2d_render/layer_10="UI"

[rendering]

textures/canvas_textures/default_texture_filter=0
textures/canvas_textures/default_texture_repeat=1
